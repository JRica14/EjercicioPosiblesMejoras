<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="290dp"
        android:layout_height="245dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="160dp"
        android:layout_marginEnd="61dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/monta_a" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="226dp"
        android:layout_height="231dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="100dp"
        android:background="#05FFFFFF"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/dado"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/play"
        android:layout_width="38dp"
        android:layout_height="40dp"
        android:layout_marginStart="148dp"
        android:layout_marginTop="112dp"
        android:background="#00FFFFFF"
        android:scaleType="centerInside"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/reanudar" />

    <ImageButton
        android:id="@+id/volver"
        android:layout_width="31dp"
        android:layout_height="40dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="112dp"
        android:background="#00FFFFFF"
        android:scaleType="centerInside"
        app:layout_constraintStart_toEndOf="@+id/pausar"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/volver" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="178dp"
        android:layout_marginTop="68dp"
        android:layout_marginEnd="179dp"
        android:text="Stranger"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/pausar"
        android:layout_width="42dp"
        android:layout_height="35dp"
        android:layout_marginStart="146dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="44dp"
        android:background="#00FFFFFF"
        android:scaleType="centerInside"
        android:visibility="invisible"
        app:layout_constraintEnd_toStartOf="@+id/volver"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:srcCompat="@drawable/pausar" />

</androidx.constraintlayout.widget.ConstraintLayout>
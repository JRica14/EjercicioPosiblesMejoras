<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="227dp"
        android:layout_height="262dp"
        android:layout_marginStart="92dp"
        android:layout_marginTop="152dp"
        android:layout_marginEnd="92dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/objeto" />

    <Button
        android:id="@+id/button"
        android:layout_width="136dp"
        android:layout_height="49dp"
        android:layout_marginStart="56dp"
        android:layout_marginBottom="199dp"
        android:text="Recoger"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button2"
        android:layout_width="136dp"
        android:layout_height="50dp"
        android:layout_marginEnd="37dp"
        android:layout_marginBottom="198dp"
        android:text="Continuar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />
    <ImageButton
        android:id="@+id/play"
        android:layout_width="38dp"
        android:layout_height="40dp"
        android:layout_marginStart="148dp"
        android:layout_marginTop="112dp"
        android:background="#00FFFFFF"
        android:scaleType="centerInside"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/reanudar" />

    <ImageButton
        android:id="@+id/volver"
        android:layout_width="31dp"
        android:layout_height="40dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="112dp"
        android:background="#00FFFFFF"
        android:scaleType="centerInside"
        app:layout_constraintStart_toEndOf="@+id/pausar"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/volver" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="178dp"
        android:layout_marginTop="68dp"
        android:layout_marginEnd="179dp"
        android:text="Stranger"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/pausar"
        android:layout_width="42dp"
        android:layout_height="35dp"
        android:layout_marginStart="146dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="44dp"
        android:background="#00FFFFFF"
        android:scaleType="centerInside"
        android:visibility="invisible"
        app:layout_constraintEnd_toStartOf="@+id/volver"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:srcCompat="@drawable/pausar" />
</androidx.constraintlayout.widget.ConstraintLayout>